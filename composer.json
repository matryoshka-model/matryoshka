{
    "name": "matryoshka-model/matryoshka",
    "description": "Model Service Layer that normalize and standardize your model's interface use",
    "homepage": "https://github.com/matryoshka-model/matryoshka",
    "minimum-stability": "dev",
    "license": "BSD-2-Clause",
    "require": {
        "php": ">=5.4.0",
        "zendframework/zend-inputfilter": "2.*",
		"zendframework/zend-stdlib": "2.*",
		"zendframework/zend-paginator": "2.*",
		"zendframework/zend-servicemanager": "2.*"
    },
    "require-dev": {
        "phpunit/phpunit": "4.0.*",
        "phpmd/phpmd": "1.5.*",
        "satooshi/php-coveralls": "dev-master",
        "fabpot/php-cs-fixer": "dev-master",
        "zendframework/zendframework": "dev-master"
    },
    "authors": [
        {
            "name": "Leonardo Grasso",
            "email": "me@leonardograsso.com"
        },
        {
            "name": "Leo Di Donato",
            "email": "leodidonato@gmail.com"
        },
        {
            "name": "Antonio Visalli",
            "email": "antonino.visalli@gmail.com"
        },
        {
            "name": "Lorenzo Fontana",
            "email": "fontanalorenzo@me.com"
        }
    ],
    "autoload": {
        "psr-0": {
            "Matryoshka": "library/",
            "MatryoshkaTest": "tests/"
        },
        "classmap": [
            "./"
        ]
    },
    "keywords": [
        "model",
        "mvc",
        "matryoshka",
        "back-end",
        "db",
        "database"
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "0.2-dev",
            "dev-develop": "0.3-dev"
        }
    }
}